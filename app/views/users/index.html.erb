<section id="user-detail" class="scroll-page bg-white">
  <div class="page-header">
    <h1>会員情報</h1>
  </div>
  <table>
    <tr>
      <th>メールアドレス</th>
      <td><%= current_user.email %></td>
    </tr>
    <tr>
      <th>お名前</th>
      <td><%= current_user.username %></td>
    </tr>
  </table>
</section>

<% @users.each do |user| %>
  <% if user.id != current_user.id %>
    <li>
      <div class="item">
        <%= link_to user.username, user %>
      </div>
    </li>
  <% end %>
<% end %>
